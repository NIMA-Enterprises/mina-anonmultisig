{
	"name": "anonmultisig",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"start": "vite --mode prod",
		"start:https": "DEBUG=vite:plugin:mkcert HTTPS=true vite --mode prod --host",
		"build": "yarn run clean:vite && vite build --mode prod",
		"predeploy": "yarn run build",
		"deploy:staging": "yarn run predeploy && firebase use staging && firebase deploy",
		"deploy:prod": "yarn run predeploy && firebase use prod && firebase deploy",
		"lint": "TIMING=1 eslint src/**/*.ts* --fix",
		"lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
		"clean": "yarn clean:vite && yarn clean:yarn",
		"clean:vite": "rimraf ./.cache ./dist",
		"clean:yarn": "rimraf ./node_modules"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.4",
		"@heroicons/react": "^2.0.13",
		"@reduxjs/toolkit": "^1.9.1",
		"backend-service-organizations": "*",
		"backend-service-vote": "*",
		"buffer": "^6.0.3",
		"classnames": "^2.3.1",
		"contract-service-anonmultisig": "*",
		"flowbite": "^1.5.5",
		"flowbite-react": "^0.3.7",
		"framer-motion": "^6.4.3",
		"hybrid-service-vote": "*",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^3.1.4",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.4.4",
		"react-toastify": "^9.0.7",
		"usehooks-ts": "^2.6.0",
		"wagmi": "0.8.7",
		"wallet-connection": "*",
		"zod": "^3.19.1"
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "^0.1.1",
		"@types/prettier": "^2.6.3",
		"@types/react": "^18.0.15",
		"@vitejs/plugin-react": "^1.3.2",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"autoprefixer": "^10.4.13",
		"cross-env": "^7.0.3",
		"postcss": "^8.4.19",
		"rimraf": "^3.0.2",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"rollup-plugin-visualizer": "^5.6.0",
		"tailwindcss": "^3.2.4",
		"typescript": "^4.7.4",
		"vite": "^2.9.14",
		"vite-plugin-chunk-split": "^0.2.4",
		"vite-plugin-imagemin": "^0.6.1",
		"vite-plugin-mkcert": "^1.7.2",
		"vite-plugin-server-headers": "^0.0.0",
		"vite-plugin-svgr": "^2.2.0",
		"vite-tsconfig-paths": "^3.3.17"
	}
}
